<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <!-- Allow JIT for V8 / WASM -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>

    <!-- Allow unsigned executable memory when needed for JIT/WASM -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>

    <!-- Allow certain DYLD env vars if your app needs them (e.g. debugging) -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>

    <!-- Disable library validation to allow loading native modules or third-party libraries -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>

    <!-- Network access (client) -->
    <key>com.apple.security.network.client</key>
    <true/>

    <!-- Allow user to select files for read/write access (e.g. attachments) -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>

    <!-- Allow direct read/write to the Downloads folder if your app needs it -->
    <key>com.apple.security.files.downloads.read-write</key>
    <true/>
  </dict>
</plist>
